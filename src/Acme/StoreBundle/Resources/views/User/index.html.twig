{% extends "StoreBundle::layout.html.twig" %}

{% block title %}<h2>Users</h2>{% endblock %}

{% block content %}
<div class="row">
	<div class="col-lg-12">
		<table class="table table-striped">
			<thead>
				<tr>
					<th>ID</th>
					<th>Username</th>
					<th>Email</th>
					<th>Action</th>
				</tr>
			</thead>
			<tbody>
				{% for user in users %}
				<tr>
					<td>{{ user.id }}</td>
					<td>{{ user.username }}</td>
					<td>{{ user.email }}</td>
					<td>Action</td>
				</tr>
				{% endfor %}
			</tbody>
		</table>
	</div>
</div>	
{% endblock %}
